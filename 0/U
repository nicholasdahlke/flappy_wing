FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (5 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
	value		$internalField;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    "(sides|top_bottom|inner)"
    {
        type            slip;
    }
    
    airfoil
    {
        type            movingWallVelocity; // Could be incorrect because of the moving wing
	value 		uniform (0 0 0);
    }
    
    AMI1
    {
        type            cyclicAMI;
	value		$internalField;
    }
    
    AMI2
    {
        type            cyclicAMI;
	value		$internalField;
    }
}


// ************************************************************************* //
