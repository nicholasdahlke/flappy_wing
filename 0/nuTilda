FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 7.5e-5;

boundaryField
{
    inlet
    {
        type            fixedValue;
	value		$internalField;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    sides
    {
        type            zeroGradient;
    }

    top_bottom
    {
	type zeroGradient;
    }
    
    airfoil
    {
        type            fixedValue;
	value		uniform 0;
    }
    
    AMI1
    {
        type            cyclicAMI;
	value		$internalField;
    }
    
    AMI2
    {
        type            cyclicAMI;
	value		$internalField;
    }
}


// ************************************************************************* //
